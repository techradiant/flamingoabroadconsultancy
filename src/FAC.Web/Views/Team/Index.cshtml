@model IEnumerable<FAC.Web.Models.Employee>
@{
    ViewBag.Title = "Index";
}

<!-- Start main-content -->
<section ction class="page-title" style="background-image: url(../../media/images/background/page-title.jpg);">
    <div class="auto-container">
        <div class="title-outer">
            <h1 class="title">Our Team </h1>
            <ul class="page-breadcrumb">
                <li>@Html.ActionLink("Home", "Index", "Home", new { area = "" }, new { @class = "" })</li>
                
                <li>Our Team </li>
            </ul>
        </div>
    </div>
</section>
<!-- end main-content -->
<!-- Team Section -->
<section class="team-section pb-60">
