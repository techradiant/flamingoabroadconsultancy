                <!-- Team block -->
                <div class="team-block col-lg-4 col-md-6 col-sm-12 wow fadeInUp">
                    <div class="inner-box">
                        <div class="image-box">
                            <figure class="image"><a href="@Url.Action("TeamDetails", "Team", new { area = "", @teamMemberId = teamMember.EmployeeId })"><img src="@teamMember.ProfilePhotoUrl" alt=""></a></figure>
                            <div class="social-links">
                                <a href="#"><i class="fab fa-twitter"></i></a>
                                <a href="#"><i class="fab fa-facebook"></i></a>
                                <a href="#"><i class="fab fa-instagram"></i></a>
                            </div>
                        </div>
                        <div class="info-box">
                            <span class="designation">@teamMember.EmployeeDesignation</span>
                            <h4 class="name"><a href="@Url.Action("TeamDetails", "Team", new { area = "", @teamMemberId = teamMember.EmployeeId })"> Hariom Kaushik </a></h4>
                        </div>
                    </div>
                </div>
            }



